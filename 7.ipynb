   "source": [
    "def moveZeroes(nums):\n",
    "    nonzero_pos = 0\n",
    "    for i in range(len(nums)):\n",
    "        if nums[i] != 0:\n",
    "            nums[i], nums[nonzero_pos] = nums[nonzero_pos], nums[i]\n",
    "            nonzero_pos += 1\n",
    "nums = [0,1,0,3,12]\n",
    "moveZeroes(nums)\n",
    "print(nums)\n"
   ]
